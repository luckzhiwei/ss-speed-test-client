<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vecent.ssspeedtest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.SpeedTestActivity"></activity>
        <service android:name=".service.TestService"></service>
        <activity android:name=".controller.InputSSServerSettingActivity"></activity>
        <activity android:name=".controller.ServiceSpeedResultActivity"/>

        <service
            android:name=".service.SpeedTestService"
            android:process=":remote"></service>
    </application>


</manifest>